{
  "name": "scrapbook",
  "version": "0.2.0",
  "description": "scrapbook is a web app to store and browse your best memories.",
  "main": "./dist/main.js",
  "scripts": {
    "dev": "npm-run-all -r -p start electron:dev",
    "prod": "npm-run-all build:renderer electron:prod",
    "electron:dev": "npm-run-all build:main:dev electron",
    "electron:prod": "npm-run-all build:main:prod electron",
    "electron": "electron .",
    "start": "webpack-dev-server --config webpack.dev.js --hot --inline --port 3000",
    "build": "npm-run-all build:main:prod build:renderer",
    "build:main:dev": "webpack --config webpack.electron.dev.js",
    "build:main:prod": "webpack --config webpack.electron.prod.js",
    "build:renderer": "webpack --config webpack.prod.js --bail"
  },
  "repository": "https://github.com/tzyl/scrapbook",
  "author": "Timothy Leung",
  "license": "MIT",
  "dependencies": {
    "@blueprintjs/core": "^2.0.0-beta.1",
    "@blueprintjs/datetime": "^2.0.0-beta.1",
    "@types/emoji-mart": "^2.2.0",
    "@types/image-size": "^0.0.29",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "@types/react-modal": "^3.1.1",
    "@types/react-redux": "^5.0.14",
    "@types/redux": "^3.6.0",
    "emoji-mart": "^2.4.2",
    "image-size": "^0.6.2",
    "localforage": "^1.5.5",
    "moment": "^2.20.1",
    "node-uuid": "^1.4.8",
    "pica": "^4.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-event-timeline": "^1.4.1",
    "react-images": "^0.5.14",
    "react-modal": "^3.1.10",
    "react-photo-gallery": "^6.0.22",
    "react-redux": "^5.0.6",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-persist": "^5.4.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^9.3.0",
    "@types/redux-devtools": "^3.0.42",
    "@types/redux-devtools-dock-monitor": "^1.1.31",
    "@types/redux-devtools-log-monitor": "^1.0.32",
    "@types/redux-logger": "^3.0.5",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "css-loader": "^0.28.7",
    "electron": "^1.8.2",
    "electron-packager": "^11.0.0",
    "electron-react-devtools": "^0.5.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.0.9",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-logger": "^3.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.1",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
