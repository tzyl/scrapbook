{
  "name": "scrapbook",
  "version": "1.0.0",
  "description": "scrapbook is a web app to store and browse your best memories.",
  "main": "./dist/main.js",
  "scripts": {
    "electron": "npm run build && electron .",
    "electron-dev": "webpack --config webpack.electron.dev.js && electron .",
    "start": "webpack-dev-server --config webpack.dev.js --hot --inline --port 3000 --open",
    "build": "npm run build-main && npm run build-renderer",
    "build-main": "webpack --config webpack.electron.prod.js",
    "build-renderer": "webpack --config webpack.prod.js --bail",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": "https://github.com/tzyl/scrapbook",
  "author": "Timothy Leung",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "@types/react-modal": "^3.1.1",
    "@types/react-redux": "^5.0.14",
    "@types/redux": "^3.6.0",
    "localforage": "^1.5.5",
    "node-uuid": "^1.4.8",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-event-timeline": "^1.4.1",
    "react-images": "^0.5.14",
    "react-modal": "^3.1.10",
    "react-photo-gallery": "^6.0.22",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-persist": "^5.4.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.3",
    "@storybook/addon-links": "^3.3.3",
    "@storybook/react": "^3.3.3",
    "@types/node": "^9.3.0",
    "@types/redux-devtools": "^3.0.42",
    "@types/redux-devtools-dock-monitor": "^1.1.31",
    "@types/redux-devtools-log-monitor": "^1.0.32",
    "@types/redux-logger": "^3.0.5",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-links": "^3.0.2",
    "@types/storybook__react": "^3.0.6",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "css-loader": "^0.28.7",
    "electron": "^1.7.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "postcss-loader": "^2.0.9",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-logger": "^3.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.1",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
